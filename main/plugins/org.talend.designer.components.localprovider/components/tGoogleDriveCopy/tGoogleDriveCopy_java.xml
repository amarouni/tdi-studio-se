<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20140417A"
    STARTABLE="true"
    
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Cloud/Google Drive</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    
	<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK" NUM_ROW="10">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST"  FILTER="tGoogleDriveConnection" NUM_ROW="10"  DYNAMIC_SETTINGS="true" SHOW_IF="USE_EXISTING_CONNECTION == 'true'" />

    <PARAMETER  NAME="APPLICATION_NAME"  FIELD="TEXT"  NUM_ROW="30" REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'" GROUP="CONNECTION_GROUP">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER  NAME="ACCESS_TOKEN"  FIELD="TEXT"  NUM_ROW="40" REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'" GROUP="CONNECTION_GROUP">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER  NAME="USE_PROXY"  FIELD="CHECK"  NUM_ROW="50" REQUIRED="true"  SHOW_IF="USE_EXISTING_CONNECTION == 'false'" GROUP="CONNECTION_GROUP">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER  NAME="PROXY_HOST"  FIELD="TEXT"  NUM_ROW="55" SHOW_IF="(USE_PROXY == 'true') AND (USE_EXISTING_CONNECTION == 'false')"  GROUP="CONNECTION_GROUP">
      <DEFAULT>"127.0.0.1"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER  NAME="PROXY_PORT"  FIELD="TEXT"  NUM_ROW="55" SHOW_IF="(USE_PROXY == 'true') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="CONNECTION_GROUP">
      <DEFAULT>8087</DEFAULT>
    </PARAMETER>
    
    <PARAMETER  NAME="USE_SSL"  FIELD="CHECK"  NUM_ROW="60" REQUIRED="true"  SHOW_IF="USE_EXISTING_CONNECTION == 'false'" GROUP="CONNECTION_GROUP">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="SSL_ALGORITHM" FIELD="TEXT" NUM_ROW="65"
		SHOW_IF="(USE_SSL == 'true') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="CONNECTION_GROUP">
      <DEFAULT>"SSL"</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="SSL_TRUST_STORE" FIELD="FILE" NUM_ROW="65"
		SHOW_IF="(USE_SSL == 'true') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="CONNECTION_GROUP">
	</PARAMETER>
	<PARAMETER NAME="SSL_TRUST_STORE_PASSWORD" FIELD="PASSWORD" NUM_ROW="65"
		SHOW_IF="(USE_SSL == 'true') AND (USE_EXISTING_CONNECTION == 'false')"  GROUP="CONNECTION_GROUP">
	</PARAMETER>

	
    <PARAMETER NAME="FILE_MODE" FIELD="RADIO" NUM_ROW="67"  GROUP="MODE">
			<DEFAULT>true</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="FOLDER_MODE" FIELD="RADIO" NUM_ROW="67"  GROUP="MODE">
       	<DEFAULT>false</DEFAULT>
	</PARAMETER>		


	<PARAMETER NAME="FILE_ID_LABEL" FIELD="LABEL" NUM_ROW="70" SHOW_IF="FILE_MODE == 'true'" GROUP="MODE">
		<DEFAULT>File Id to copy</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="FOLDER_ID_LABEL" FIELD="LABEL" NUM_ROW="70" SHOW_IF="FOLDER_MODE == 'true'" GROUP="MODE">
		<DEFAULT>Folder Id to move</DEFAULT>
	</PARAMETER>

    <PARAMETER NAME="FILE_ID"  FIELD="TEXT" NUM_ROW="71" REQUIRED="true" GROUP="MODE">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="SOURCE_FOLDERID"  FIELD="TEXT"  NUM_ROW="74" REQUIRED="true"  SHOW_IF="FOLDER_MODE == 'true'" GROUP="MODE">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
    <PARAMETER NAME="DESTINATION_FOLDERID"  FIELD="TEXT"  NUM_ROW="75" REQUIRED="true" GROUP="MODE">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="RENAME" FIELD="CHECK" NUM_ROW="80" GROUP="MODE">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="DESTINATION_FILE_NAME"  FIELD="TEXT"  NUM_ROW="85" REQUIRED="true" SHOW_IF="(RENAME == 'true')" GROUP="MODE">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="REMOVE_SOURCE_FILE" FIELD="CHECK" NUM_ROW="90" SHOW_IF="FILE_MODE == 'true'" GROUP="MODE">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="95">
		<TABLE READONLY="true">
			<COLUMN NAME="sourceFileID" TYPE="id_String" />
			<COLUMN NAME="destinationFileID" TYPE="id_String" />
		</TABLE>
	</PARAMETER>
    
    <PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="100">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

   </PARAMETERS>
   
	<ADVANCED_PARAMETERS>
	</ADVANCED_PARAMETERS>
	
   <CODEGENERATION>
      	<IMPORTS>
		</IMPORTS>
   </CODEGENERATION>

  <RETURNS>
  	<RETURN NAME="SOURCE_FILEID" TYPE="id_String" AVAILABILITY="AFTER" />
	<RETURN NAME="DESTINATION_FILEID" TYPE="id_String" AVAILABILITY="AFTER" />
  </RETURNS>
</COMPONENT>