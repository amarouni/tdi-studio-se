<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20140417A"
    STARTABLE="true"
    
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Cloud/Google Drive</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="RUN_IF" MAX_INPUT="1"/>
  </CONNECTORS>

  <PARAMETERS>
  
  	<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK" NUM_ROW="10">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST"  FILTER="tGoogleDriveConnection" NUM_ROW="10"  DYNAMIC_SETTINGS="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"/>

    <PARAMETER  NAME="APPLICATION_NAME"  FIELD="TEXT"  NUM_ROW="30" REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'" GROUP="CONNECTION_GROUP">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER  NAME="ACCESS_TOKEN"  FIELD="TEXT"  NUM_ROW="40" REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'" GROUP="CONNECTION_GROUP">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER  NAME="USE_PROXY"  FIELD="CHECK"  NUM_ROW="50" REQUIRED="true"  SHOW_IF="USE_EXISTING_CONNECTION == 'false'" GROUP="CONNECTION_GROUP">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER  NAME="PROXY_HOST"  FIELD="TEXT"  NUM_ROW="55" SHOW_IF="(USE_PROXY == 'true') AND (USE_EXISTING_CONNECTION == 'false')"  GROUP="CONNECTION_GROUP">
      <DEFAULT>"127.0.0.1"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER  NAME="PROXY_PORT"  FIELD="TEXT"  NUM_ROW="55" SHOW_IF="(USE_PROXY == 'true') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="CONNECTION_GROUP">
      <DEFAULT>8087</DEFAULT>
    </PARAMETER>
    
    <PARAMETER  NAME="USE_SSL"  FIELD="CHECK"  NUM_ROW="60" REQUIRED="true"  SHOW_IF="USE_EXISTING_CONNECTION == 'false'" GROUP="CONNECTION_GROUP">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="SSL_ALGORITHM" FIELD="TEXT" NUM_ROW="65"
		SHOW_IF="(USE_SSL == 'true') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="CONNECTION_GROUP">
      <DEFAULT>"SSL"</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="SSL_TRUST_STORE" FIELD="FILE" NUM_ROW="65"
		SHOW_IF="(USE_SSL == 'true') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="CONNECTION_GROUP">
	</PARAMETER>
	<PARAMETER NAME="SSL_TRUST_STORE_PASSWORD" FIELD="PASSWORD" NUM_ROW="65"
		SHOW_IF="(USE_SSL == 'true') AND (USE_EXISTING_CONNECTION == 'false')"  GROUP="CONNECTION_GROUP">
	</PARAMETER>

    <PARAMETER NAME="FILE_TITLE"  FIELD="TEXT"  NUM_ROW="70">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="DESTINATION_FOLDERID"  FIELD="TEXT"  NUM_ROW="75" REQUIRED="true">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="78">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="READ_CONTENT_FROM_INPUT" FIELD="RADIO" NUM_ROW="80" GROUP="UPLOAD_MODE">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
	<PARAMETER NAME="UPLOAD_LOCAL_FILE" FIELD="RADIO" NUM_ROW="80" GROUP="UPLOAD_MODE">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
<!-- 	<PARAMETER NAME="EXPOSE_OUTPUT_STREAM" FIELD="RADIO" NUM_ROW="80" GROUP="UPLOAD_MODE">
      <DEFAULT>false</DEFAULT>
    </PARAMETER> -->
	<PARAMETER NAME="LOCAL_FILE_PATH" FIELD="FILE" NUM_ROW="86" SHOW_IF="UPLOAD_LOCAL_FILE == 'true' " GROUP="UPLOAD_MODE">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="99" SHOW_IF="READ_CONTENT_FROM_INPUT == 'true'" GROUP="UPLOAD_MODE">
		<TABLE READONLY="true">
			<COLUMN NAME="content" TYPE="id_Object" />
		</TABLE>
	</PARAMETER>	    
   </PARAMETERS>
   
	<ADVANCED_PARAMETERS>
	</ADVANCED_PARAMETERS>
	
   <CODEGENERATION>
      	<IMPORTS>
      	<!-- same as tGoogleDriveConnection -->
    		   <IMPORT NAME="google-api-client"
                    MODULE="google-api-client-1.19.0.jar"
                    UrlPath="platform:/plugin/org.talend.designer.components.localprovider/components/tGoogleDriveConnection/google-api-client-1.19.0.jar"
                    REQUIRED="true" />
               <IMPORT NAME="google-api-services-drive-v2"
                    MODULE="google-api-services-drive-v2-rev144-1.19.0.jar"
                    UrlPath="platform:/plugin/org.talend.designer.components.localprovider/components/tGoogleDriveConnection/google-api-services-drive-v2-rev144-1.19.0.jar"
                    REQUIRED="true" />
               <IMPORT NAME="google-http-client"
                    MODULE="google-http-client-1.19.0.jar"
                    UrlPath="platform:/plugin/org.talend.designer.components.localprovider/components/tGoogleDriveConnection/google-http-client-1.19.0.jar"
                    REQUIRED="true" />
               <IMPORT NAME="google-http-client-jackson2"
                    MODULE="google-http-client-jackson2-1.19.0.jar"
                    UrlPath="platform:/plugin/org.talend.designer.components.localprovider/components/tGoogleDriveConnection/google-http-client-jackson2-1.19.0.jar"
                    REQUIRED="true" />
               <IMPORT NAME="google-oauth-client"
                    MODULE="google-oauth-client-1.19.0.jar"
                    UrlPath="platform:/plugin/org.talend.designer.components.localprovider/components/tGoogleDriveConnection/google-oauth-client-1.19.0.jar"
                    REQUIRED="true" />
               <IMPORT NAME="guava-jdk5"
                    MODULE="guava-jdk5-13.0.jar"
                    UrlPath="platform:/plugin/org.talend.designer.components.localprovider/components/tGoogleDriveConnection/guava-jdk5-13.0.jar"
                    REQUIRED="true" />
               <IMPORT NAME="jsr305-"
                    MODULE="jsr305-1.3.9.jar"
                    UrlPath="platform:/plugin/org.talend.designer.components.localprovider/components/tGoogleDriveConnection/jsr305-1.3.9.jar"
                    REQUIRED="true" />

               <IMPORT NAME="jackson-core"
                    MODULE="jackson-core-2.2.2.jar"
                    UrlPath="platform:/plugin/org.talend.libraries.jackson/lib/jackson-core-2.2.2.jar"
                    REQUIRED="true" />
               <IMPORT NAME="httpclient"
                    MODULE="httpclient-4.0.1.jar"
                    UrlPath="platform:/plugin/org.talend.libraries.apache.http/lib/httpclient-4.0.1.jar"
                    REQUIRED="true" />
               <IMPORT NAME="httpcore"
                    MODULE="httpcore-4.0.1.jar"
                    UrlPath="platform:/plugin/org.talend.libraries.apache.http/lib/httpcore-4.0.1.jar"
                    REQUIRED="true" />
               <IMPORT NAME="commons-logging"
                    MODULE="commons-logging-1.1.3.jar"
                    UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-logging-1.1.3.jar"
                    REQUIRED="true" />
               <IMPORT NAME="commons-codec"
                    MODULE="commons-codec-1.4.jar"
                    UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-codec-1.4.jar"
                    REQUIRED="true" />
		</IMPORTS>
   </CODEGENERATION>

  <RETURNS>
  	 <RETURN NAME="OUTPUTSTREAM" TYPE="id_Object" AVAILABILITY="FLOW" SHOW_IF="EXPOSE_OUTPUT_STREAM=='true'"/>
  </RETURNS>
</COMPONENT>